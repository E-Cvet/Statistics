   "source": [
    "# Define Y\n",
    "lmbd = 13\n",
    "Y = stats.poisson(lmbd)\n",
    "\n",
    "n = 25\n",
    "df = n-1\n",
    "T = stats.t(df=df)\n",
    "\n",
    "print('95% CI')\n",
    "c_level = 0.95\n",
    "for i in range(10): # loop\n",
    "    sample = Y.rvs(size=n)\n",
    "    x_bar = sample.mean()\n",
    "    se = sample.std()/np.sqrt(n)\n",
    "    t_star = T.ppf((1+c_level)/2)\n",
    "    l = x_bar - t_star*se\n",
    "    u = x_bar + t_star*se\n",
    "    if l<=Y.mean() and Y.mean()<=u:\n",
    "        print(f'({l}, {u})')\n",
    "    else:\n",
    "        print(f'({l}, {u}) ***')\n",
    "        \n",
    "        \n",
    "        \n",
    "print('\\n99% CI')\n",
    "c_level = 0.99\n",
    "for i in range(10): # loop\n",
    "    sample = Y.rvs(size=n)\n",
    "    x_bar = sample.mean()\n",
    "    se = sample.std()/np.sqrt(n)\n",
    "    t_star = T.ppf((1+c_level)/2)\n",
    "    l = x_bar - t_star*se\n",
    "    u = x_bar + t_star*se\n",
    "    if l<=Y.mean() and Y.mean()<=u:\n",
    "        print(f'({l}, {u})')\n",
    "    else:\n",
    "        print(f'({l}, {u}) ***')"
   ]
